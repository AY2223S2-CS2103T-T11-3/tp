@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title After command "delete 5"

package States <<rectangle>> {
    class State1 as "__pl0:EventBook__"
    class State2 as "__pl1:EventBook__"
    class State3 as "__pl2:EventBook__"
}

State1 -[hidden]right-> State2
State2 -[hidden]right-> State3

hide State3

class Pointer as "Current State" #00ff99

Pointer -up-> State2
@end
